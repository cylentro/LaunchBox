{
	"builds": [
		{
			"src": "package.json",
			"use": "@vercel/static-build",
			"config": { "outputDirectory": "docs/.vitepress/dist" }
		}
	],
	"rewrites": [
		{
			"source": "/blog/:post",
			"destination": "/blog/posts/:post.html"
		},
		{
			"source": "/:path((?!.*\\.html$).*)",
			"destination": "/:path.html"
		}
	]
}
